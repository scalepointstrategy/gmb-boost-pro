{
  "name": "gmp-profile-pulse-backend",
  "version": "1.0.0",
  "description": "Backend server for Google Business Profile integration",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:local": "set RUN_MODE=LOCAL && nodemon server.js",
    "dev:azure": "set RUN_MODE=AZURE && nodemon server.js",
    "start:local": "set RUN_MODE=LOCAL && node server.js",
    "start:azure": "set RUN_MODE=AZURE && node server.js",
    "start:production": "node start-production.js",
    "docker:build": "docker build -t gmb-boost-pro-backend:latest .",
    "docker:run": "docker-compose up -d",
    "docker:run:prod": "docker-compose -f docker-compose.production.yml up -d --build",
    "docker:stop": "docker-compose down",
    "docker:stop:prod": "docker-compose -f docker-compose.production.yml down",
    "docker:logs": "docker-compose logs -f backend",
    "docker:logs:prod": "docker-compose -f docker-compose.production.yml logs -f backend",
    "docker:push": "./docker-push.sh scale112",
    "docker:tag": "docker tag gmb-boost-pro-backend:latest scale112/gmb-boost-pro-backend:latest",
    "docker:pull": "docker pull scale112/gmb-boost-pro-backend:v6",
    "docker:run:hub": "docker-compose -f docker-compose.hub.yml up -d",
    "docker:stop:hub": "docker-compose -f docker-compose.hub.yml down"
  },
  "keywords": ["google-business-profile", "oauth", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "googleapis": "^128.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}

